# AI Service Environment Variables
# Copy this file to ai-service/.env and fill in your actual values

# RunPod API Configuration
RUNPOD_API_KEY=your_runpod_api_key_here

# Hugging Face Configuration  
HUGGINGFACE_API_KEY=your_huggingface_token_here
HF_HOME=/path/to/your/cache/huggingface/transformers
HUGGINGFACE_HUB_CACHE=/path/to/your/cache/huggingface/hub

# News API (for financial data collection)
NEWSAPI_KEY=your_newsapi_key_here
ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here
FINNHUB_KEY=your_finnhub_key_here
